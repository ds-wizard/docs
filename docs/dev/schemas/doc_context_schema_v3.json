        "logo": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "props": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "requestBody": {
          "type": "string"
        },
        "requestHeaders": {
          "$ref": "#/definitions/stringMap"
        },
        "requestMethod": {
          "type": "string"
        },
        "responseIdField": {
          "type": "string"
        },
        "responseListField": {
          "type": "string"
        },
        "responseNameField": {
          "type": "string"
        },
