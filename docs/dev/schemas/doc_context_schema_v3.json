          }
        },
        "label": {
          "type": "string"
        },
        "metricMeasures": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/metricMeasure"
          }
        },
        "uuid": {
          "$ref": "#/definitions/uuid"
        }
      },
