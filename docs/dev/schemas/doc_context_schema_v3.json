        "tagUuids": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/uuid"
          }
        },
        "text": {
          "$ref": "#/definitions/optionalString"
        },
        "title": {
          "type": "string"
        },
        "uuid": {
          "$ref": "#/definitions/uuid"
        },
        "valueType": {
          "type": "string",
          "enum": [
            "DateQuestionValueType",
            "NumberQuestionValueType",
            "StringQuestionValueType",
            "TextQuestionValueType"
          ]
        }
